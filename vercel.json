{
  "version": 2,
  "builds": [
    {
      "src": "./src/*",
      "use": "@vercel/node"
    },
      {
      "src": "./src/middleware/*",
      "use": "@vercel/node"
    },
        {
      "src": "./src/models/*",
      "use": "@vercel/node"
    },
        {
      "src": "./src/routes/*",
      "use": "@vercel/node"
    },
 
        {
      "src": "./src/services/*",
      "use": "@vercel/node"
    }
    
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/src/server.js"
    }
  ]
}
